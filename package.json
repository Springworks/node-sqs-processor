{
  "name": "sqs-processor",
  "version": "1.0.1",
  "description": "Module for SQS queue processing",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test mocha --ui bdd --check-leaks --recursive --slow 150 --reporter spec test",
    "test-cov": "NODE_ENV=test istanbul cover --report teamcity --report lcov _mocha -- --ui bdd --check-leaks --recursive --slow 150 --reporter spec test",
    "test-debug": "NODE_ENV=test node-debug _mocha --bail --ui bdd --check-leaks --recursive --reporter dot test",
    "test-changed": "NODE_ENV=test mocha --ui bdd --check-leaks --recursive --slow 150 --reporter dot $(git diff --name-only test | grep -E '(\\.js)$')",
    "lint": "eslint ."
  },
  "dependencies": {
    "async": "^0.9.0",
    "aws-sdk": "^2.0.25",
    "error-factory": "https://github.com/Springworks/node-error-factory/archive/v0.3.0.tar.gz",
    "input-validator": "https://github.com/Springworks/node-input-validator/archive/v0.5.0.tar.gz"
  },
  "devDependencies": {
    "chai": "^1.10.0",
    "eslint": "^0.9.2",
    "istanbul": "^0.3.0",
    "mocha": "^2.0.1",
    "sinon": "^1.11.1",
    "sinon-chai": "^2.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Springworks/node-sqs-processor.git"
  },
  "bugs": {
    "url": "https://github.com/Springworks/node-sqs-processor/issues"
  },
  "homepage": "https://github.com/Springworks/node-sqs-processor",
  "author": "Springworks",
  "license": "MIT"
}
